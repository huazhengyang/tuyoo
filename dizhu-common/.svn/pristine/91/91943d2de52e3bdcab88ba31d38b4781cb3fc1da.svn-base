from dizhucomm.utils.obser import EventBase


class RoomEvent(EventBase):
    def __init__(self, room):
        super(RoomEvent, self).__init__()
        self.room = room


class RoomConfReloadEvent(RoomEvent):
    def __init__(self, room):
        super(RoomConfReloadEvent, self).__init__(room)


